{
    "name": "ruvents/abstract-api-client",
    "description": "RUVENTS Abstract Api Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ruvents\\AbstractApiClient\\": "lib/"
        }
    },
    "require": {
        "php": "^5.4 || ^7.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "~0.6.4 || ^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/event-dispatcher": "^2.8 || ^3.0",
        "symfony/options-resolver": "^2.8 || ^3.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "symfony/serializer": "^2.8 || ^3.0"
    },
    "config": {
        "platform": {
            "php": "5.5.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
